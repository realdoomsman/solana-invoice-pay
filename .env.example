# Solana Network (devnet, testnet, mainnet-beta)
NEXT_PUBLIC_SOLANA_NETWORK=devnet

# Supabase (Get from https://supabase.com)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Encryption Key (Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
ENCRYPTION_KEY=your_random_32_character_encryption_key_here

# Platform Fee (Your Revenue)
PLATFORM_FEE_PERCENTAGE=1.5
PLATFORM_FEE_WALLET=your_wallet_address_to_receive_fees

# Email Notifications (Optional - Get from https://sendgrid.com)
SENDGRID_API_KEY=your_sendgrid_api_key_here
NOTIFICATION_EMAIL=your_email@example.com
